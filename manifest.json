{
    "manifest_version": 3,
    "name": "Prior Authorization Autoupload",
    "version": "0.1",
    "description": "Auto dowload, rename, and upload faxed PA from covermymeds into emd.md",
    "permissions": [
        "tabs", 
        "cookies",
        "storage",
        "downloads",
        "scripting",
        "webRequest",
        "webNavigation"
    ],
    "host_permissions": ["https://dashboard.covermymeds.com/*"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        
    }
}